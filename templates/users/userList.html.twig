{% extends 'base.html.twig' %}

{% block body %}
<div class="container">
{% for user in users %}
    <div class="row" >
        <div class="col-2" style="border-style: ridge">
            ID: {{ user.id }}
        </div>
        <div class="col-9" style="border-style: ridge">
            <a href="user/{{ user.id }}">{{ user.username }}</a>
        </div>
    </div>



{% endfor %}
</div>
{% endblock %}