{% extends '_base.html.twig' %}
{% block body %}
    <div class="container">
        <div class="row">
            <div class="col-4">
                <a href="{{ path('admin') }}" class="badge badge-info">Admin
                </a>
            </div>
        </div>
        {% for user in users %}
            <div class="list-group">
                <div class="row">
                    <div class="col-3">
                        <a class="list-group-item list-group-item-action">ID: {{ user.id }}
                        </a>
                    </div>
                    <div class="col-7">
                        <a class="list-group-item list-group-item-action">{{ user.username }}
                        </a>
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock %}
